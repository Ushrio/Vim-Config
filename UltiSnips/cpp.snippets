# Look for C snippets as well
extends c

# Snippet to create main function
snippet main "Create main function"
int main() {
	$0

	return 1;
}
endsnippet

# Snippet to create class
snippet class "Create class"
class $1 {
	$0
};
endsnippet

# Snippet for cout
snippet cout "Output" iA
cout << $0
endsnippet
